  <img src="http://students.flatironschool.com/<%= @student.profile_image %>" width="300">




  <!-- show attributes for a single student here -->

   <h1><%= @student.name %></h1>
   <dl class="dl-horizontal">
      <dt>Quote</dt>
      <dd><%= @student.quote %></dd>
      <dt>Bio</dt>
      <dd><%= @student.bio %></dd>
      <dt>Education</dt>
      <dd><%= @student.education %></dd>
      <dt>Work</dt>
      <dd><%= @student.work %></dd>
      <dt>Work title</dt>
      <dd><%= @student.work_title %></dd>
      <dt>Personal Projects</dt>
      <dd><%= @student.personal_project %><dd>
      <dt>Social Media</dt>
      <dd><a href="<%= @student.twitter %>" target=_blank>Twitter</a></dd>
      <dd><a href="<%= @student.linkedin %>" target=_blank>LinkedIn</a></dd>
      <dd><a href="<%= @student.github %>" target=_blank>Github</a></dd>
  </dl>

  <a class="btn btn-default" href="/students">Back</a> 
  <a class="btn btn-default" href="/students/<%= @student.slug %>/edit">Edit</a>  
  <a class="btn btn-danger" href="/students/<%= @student.slug %>/destroy">Destroy</a>
